<!-- Image description generated by {{ data['generator'] }} on {{ data['timestamp'] }} -->

{% if data['profiles']|length > 1 -%}
{%- set multibuild = True -%}
<!-- OBS-Profiles: @BUILD_FLAVOR@ -->
{% endif -%}
<!-- OBS-ExclusiveArch: {{ ' '.join(data['archs']) }} -->

<image schemaversion="{{ data['image']['_attributes']['schemaversion'] }}" name="{{ data['image']['description']['name']}}" displayname="{{ data['image']['name'] }}">
    <description type="system">
        <author>{{ data['image']['description']['author'] }}</author>
        <contact>{{ data['image']['description']['contact'] }}</contact>
        <specification>{{ data['image']['specification'] }}</specification>
    </description>
</image>
